{
  "name": "webpack-react-redux-server-side-render-docker-example",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:build:* dev:server ",
    "dev:server": "nodemon build/bundle.js",
    "dev:build:server": "webpack --mode development --config webpack.server.js --watch",
    "dev:build:client": "webpack --mode development --config webpack.client.dev.js --watch",
    "build": "npm run build:client && npm run build:server",
    "build:server": "webpack --mode development --config webpack.server.js",
    "build:client": "webpack --mode development --config webpack.client.prod.js",
    "lint": "eslint src/*.js --quiet",
    "start": "node ./build/bundle.js",
    "docker:refresh": "docker-compose down -v && docker-compose build --no-cache",
    "docker:up": "docker-compose up"
  },
  "dependencies": {
    "big.js": "^6.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "formask": "^0.3.3",
    "http-proxy-middleware": "^2.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "json5": "^2.2.0",
    "lodash.throttle": "^4.1.1",
    "path-to-regexp": "^6.2.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-scrolllock": "^5.0.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.14.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  }
}
